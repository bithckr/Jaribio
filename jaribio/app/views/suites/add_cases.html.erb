<div class="block">
  <%= render :partial => "partials/navigation", :locals => { :active => nil } %>
  <div class="content">
    <div class="title"><%= t('jaribio.actions.editing', :default => 'Editing') %> <%= t('jaribio.objects.suite', :default => 'Suite') %>: <%= @suite.name %></div>
    <div class="inner">
      <div class="title"><%= t('jaribio.actions.associate', :default => 'Associate') %> <%= t('jaribio.objects.cases', :default => 'Cases') %></div>
      <%= render :partial => "partials/search", :locals => { :action => 'add_cases' } %>
      <div class="inner">
        <%= render :partial => "test_cases/test_case_list", :locals => { :test_cases => @new_cases, :menu => 'suites/new_cases_item_menu' } %>
        <div class="actions-bar wat-cf">
          <div class="actions">
          </div>
          <div class="pagination">
            <%= paginate @new_cases %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
