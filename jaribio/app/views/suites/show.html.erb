<div class="block">
  <%= render :partial => "partials/navigation", :locals => { :active => nil } %>
  <div class="content">
    <div class="title"><%= t('jaribio.actions.viewing', :default => 'Viewing') %> <%= t('jaribio.objects.suite', :default => 'Suite') %></div>
    <div class="inner">
      <%= render :partial => 'form', :locals => { :disabled => true } %>
    </div>
    <div class="title">Current Cases</div>
    <div class="inner">
      <%= render :partial => "test_cases/test_case_list", :locals => { :test_cases => @current_cases, :menu => 'suites/current_cases_view_item_menu' } %>
      <div class="actions-bar wat-cf">
        <div class="actions">
        </div>
        <div class="pagination">
        </div>
      </div>
    </div>
  </div>
</div>
